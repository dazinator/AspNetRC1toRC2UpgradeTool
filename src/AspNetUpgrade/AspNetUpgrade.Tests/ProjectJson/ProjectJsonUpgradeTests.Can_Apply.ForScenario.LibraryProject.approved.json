      "copy /y '%project:Directory%\\..\\..\\artifacts\\bin\\%project:Name%\\%build:Configuration%\\%project:Name%.%project:Version%.nupkg' '%project:Directory%\\..\\..\\artifacts\\bin\\Modules\\%project:Name%.%project:Version%.nupkg"
    
