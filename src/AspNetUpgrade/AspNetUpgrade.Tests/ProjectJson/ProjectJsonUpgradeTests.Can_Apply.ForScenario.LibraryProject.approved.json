    "netstandard1.5":  {
            
      "imports":  [
                
        "dnxcore50",
                
        "portable-net45+win8"
      
            ],
            
      "dependencies":  {
                
        "NETStandard.Library":  "1.5.0-rc2-24027"
      
            }
    
        }
  
    },
    
  "packInclude":  {
        
